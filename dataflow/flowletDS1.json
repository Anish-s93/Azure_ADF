{
	"name": "flowletDS1",
	"properties": {
		"type": "Flowlet",
		"typeProperties": {
			"sources": [],
			"sinks": [],
			"transformations": [
				{
					"name": "aggregate1"
				},
				{
					"name": "input1"
				},
				{
					"name": "output1"
				}
			],
			"scriptLines": [
				"input(output(",
				"          depid as integer,",
				"          depname as string",
				"     ),",
				"     order: 0,",
				"     allowSchemaDrift: true) ~> input1",
				"input1 aggregate(groupBy(depname,",
				"          depid),",
				"     Depcount = count()) ~> aggregate1",
				"aggregate1 output(mapColumn(",
				"          depname,",
				"          depid",
				"     )) ~> output1"
			]
		}
	}
}