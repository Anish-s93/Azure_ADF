				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"SqlServer1_password": {
						"value": "[parameters('SqlServer1_password')]"
					},
					"sqlserver_parameter_password": {
						"value": "[parameters('sqlserver_parameter_password')]"
					},
					"SqlServer1_properties_typeProperties_server": {
						"value": "[parameters('SqlServer1_properties_typeProperties_server')]"
					},
					"SqlServer1_properties_typeProperties_database": {
						"value": "[parameters('SqlServer1_properties_typeProperties_database')]"
					},
					"SqlServer1_properties_typeProperties_userName": {
						"value": "[parameters('SqlServer1_properties_typeProperties_userName')]"
					},
					"sqlserver_parameter_properties_typeProperties_server": {
						"value": "[parameters('sqlserver_parameter_properties_typeProperties_server')]"
					},
					"sqlserver_parameter_properties_typeProperties_database": {
						"value": "[parameters('sqlserver_parameter_properties_typeProperties_database')]"
					},
					"sqlserver_parameter_properties_typeProperties_userName": {
						"value": "[parameters('sqlserver_parameter_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'anidatafactory12_ArmTemplate_0')]"
			]
		},
		{
			"name": "anidatafactory12_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'anidatafactory12_ArmTemplate_1')]"
			]
		},
		{
			"name": "anidatafactory12_ArmTemplate_3",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_3.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ptrigger_properties_paramerizedpipeline_parameters_Tablenamefrompipeline": {
						"value": "[parameters('ptrigger_properties_paramerizedpipeline_parameters_Tablenamefrompipeline')]"
					},
					"ptrigger_properties_paramerizedpipeline_parameters_dbnamefrompipeline": {
						"value": "[parameters('ptrigger_properties_paramerizedpipeline_parameters_dbnamefrompipeline')]"
					},
					"ptrigger_properties_paramerizedpipeline_parameters_destinationtable": {
						"value": "[parameters('ptrigger_properties_paramerizedpipeline_parameters_destinationtable')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'anidatafactory12_ArmTemplate_2')]"
			]
		}
	]
}